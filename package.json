{
  "name": "hapi-locale",
  "version": "2.0.1",
  "description": "Configurable plugin for determine request language in hapi.js applications.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ozum/hapi-locale.git"
  },
  "scripts": {
    "doc": "rm -R doc; jsdoc -c jsdoc-conf.json README-JSDOC.md;    rm -f README.md;    jsdoc2md --no-gfm --src \"lib/**/*\" >JSDOC.md;    cat README-JSDOC.md JSDOC.md History.md LICENSE > README.md;    rm -r -f JSDOC.md;",
    "test": "lab --globals BigUint64Array,BigInt64Array,BigInt,Reflect,SharedArrayBuffer,Atomics,WebAssembly,URL,URLSearchParams,TextEncoder,TextDecoder,queueMicrotask"
  },
  "keywords": [
    "hapi",
    "i18n",
    "l10n",
    "request",
    "langauge",
    "locale"
  ],
  "author": "Özüm Eldoğan",
  "license": "MIT",
  "dependencies": {
    "@hapi/boom": "^9.1.0",
    "@hapi/joi": "^17.1.1",
    "accept-language-parser": "^1.5.0",
    "lodash": "^4.17.19"
  },
  "devDependencies": {
    "@hapi/code": "^8.0.1",
    "@hapi/hapi": "^19.2.0",
    "@hapi/lab": "^22.0.4",
    "rewire": "^5.0.0",
    "zoo-jsdoc": "0.0.1"
  },
  "engines": {
    "node": ">=8.3.0"
  }
}
